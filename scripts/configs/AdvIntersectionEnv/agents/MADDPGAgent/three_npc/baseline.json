{
    "__class__": "<class 'rl_agents.agents.multi_agent_deep_deterministic_policy_gradient.pytorch.MADDPGAgent'>",
    "models": {
        "actor": {
            "type":"ActorNetwork",
            "base_net": {
                "type": "MultiLayerPerceptron",
                "layers": [128, 128]
            }
        },
        "critic": {
            "type":"CriticNetwork",
            "base_net": {
                "type": "MultiLayerPerceptron",
                "layers": [128, 128, 128]
            }
        }
    },
    "n_agents": 4,
    "gamma": 0.95,
    "n_steps": 1,
    "memory_capacity": 50000,
    "target_update": 512,
    "exploration": {
        "method": "EpsilonGreedy",
        "tau": 150000,
        "temperature": 1.0,
        "final_temperature": 0.05
    }
}
