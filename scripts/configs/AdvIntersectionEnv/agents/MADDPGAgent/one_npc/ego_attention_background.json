{
    "base_config": "configs/AdvIntersectionEnv/agents/MADDPGAgent/one_npc/ego_attention_agent_background.json",
    "models": {
        "critic": {
            "type":"CriticNetwork",
            "base_net": {
                "type": "EgoAttentionNetwork",
                "embedding_layer": {
                    "type": "MultiLayerPerceptron",
                    "layers": [64, 64],
                    "reshape": false,
                    "in": 12
                },
                "others_embedding_layer": {
                    "type": "MultiLayerPerceptron",
                    "layers": [64, 64],
                    "reshape": false,
                    "in": 12
                },
                "self_attention_layer": null,
                "attention_layer": {
                    "type": "EgoAttention",
                    "feature_size": 64,
                    "heads": 1
                },
                "output_layer": {
                    "type": "MultiLayerPerceptron",
                    "layers": [64, 64],
                    "reshape": false
                }
            }
        }
    }
}